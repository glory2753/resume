<div style="padding: 20px;"> 
<nav>
  {{#each navigation.links}}
    <a class="mx-2" href="{{this.href}}">{{this.text}}</a>
  {{/each}}
  
  <a type="button" class="btn btn-primary btn-sm mx-2" href="{{navigation.button.href}}">{{navigation.button.text}}</a>
</nav>
<hr>

<header>
  <h1>{{header.title}}</h1>
  <p>{{header.description}}</p>
  <a type="button" class="btn btn-primary btn-sm" href="{{header.button.link}}">{{header.button.text}}</a>
</header>
<hr>

<section>
  <h2>{{city.title}}</h2>
  <br>
  <div class="row">
    {{#each city.cards}}
    <div class="col-6">
      <div class="card card-title">
        <img src="{{this.picture.src}}" alt="{{this.picture.alt}}">
        <div style="padding: 10px;">
          <h3>{{this.title}}</h3>
          <p>{{this.description}}</p>
          <a href="{{this.button.href}}">
            <button type="button" class="btn btn-primary">{{this.button.text}}</button>
          </a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</section>
<hr>

<section>
  <h2>{{winner.title}}</h2>
  <table class="table">
    <thead>
      <tr>
        {{#each winner.table.head}}
        <th>{{this}}</th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each winner.table.user}}
      <tr>
        <td>{{this.place}}</td>
        <td>{{this.name}}</td>
        <td>{{this.surname}}</td>
        <td>{{this.nickname}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</section>
<br>
<hr>

<section>
  <h2>{{gallery.title}}</h2>
  <br>
  <br>
<div class="d-flex">
  <div class="card col-4 mx-2">
    <ul class="list-group list-group-flush">
      <div class="card-header">
        <b class="card-title">{{gallery.cards.[0].name}}</b>
      </div>
      {{#each gallery.cards.[0].items}}
      <li class="list-group-item">{{this}}</li>
      {{/each}}
    </ul>
  </div>

  <div class="card col-4 mx-2">
    <ul class="list-group list-group-flush">
      <div class="card-header">
        <b class="card-title">{{gallery.cards.[1].name}}</b>
      </div>
      {{#each gallery.cards.[1].items}}
      <li class="list-group-item">{{this}}</li>
      {{/each}}
    </ul>
  </div>

  <div class="card col-4 mx-2">
    <ul class="list-group list-group-flush">
      <div class="card-header">
        <b class="card-title">{{gallery.cards.[2].name}}</b>
      </div>
      {{#each gallery.cards.[2].items}}
      <li class="list-group-item">{{this}}</li>
      {{/each}}
    </ul>
  </div>
</div>
</section>
<br>
<hr>

