{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/package-schema.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "version": {
        "type": "string"
      },
      "private": {
        "type": "boolean"
      },
      "scripts": {
        "type": "object"
      },
      "dependencies": {
        "type": "object"
      },
      "devDependencies": {
        "type": "object"
      }
    },
    "required": ["name", "version"]
  }
  